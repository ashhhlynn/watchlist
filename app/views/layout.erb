  <body>
    <div class="wrapper">

        <%= yield %>

      <footer class="branding">
        <small>&copy; 2022 <strong>Watchlist</strong></small>
      </footer>

<footer >
<a button type="button" href="/">Home</a>
<a button type="button" href="/users">Browse Users</a>
<a button type="button" href="/watchings">Browse Movies and Shows</a> 
<% if logged_in? %>
<% @user = current_user %>
<a button type="button" href="/users/<%= @user.id %>">Your Watchlist</a>
<a button type="button" href="/users/<%= @user.id %>/edit">Edit or Delete My Account</a> 
<a button type="button" href="/logout">Logout</a> 
<% else %>
<a button type="button" href="/signup">Signup</a>
<a button type="button" href="/login">Login</a> 
<% end %>
</footer>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
